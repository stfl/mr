[$HOME/.config/vcsh/repo.d/git.git]
checkout =
   vcsh clone git@bitbucket.org:stefanlendl/git.git || vcsh git checkout master --force
   vcsh git config branch.master.remote origin
   vcsh git config branch.master.merge refs/heads/master
   vcsh write-gitignore git
update   = vcsh git pull --rebase
push     = vcsh git push
status   = vcsh git status
gc       = vcsh git gc
