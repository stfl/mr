[$HOME/.config/vcsh/repo.d/tmux.git]
checkout =
   vcsh clone git@bitbucket.org:stefanlendl/tmux.git || vcsh tmux checkout master --force
update   =
   vcsh tmux pull --rebase
push     = vcsh tmux push
status   = vcsh tmux status --short
gc       = vcsh tmux gc

[$HOME/.tmux/plugins/tpm]
checkout = git clone https://github.com/tmux-plugins/tpm
update   = git pull
push     = return 0 # read-only
